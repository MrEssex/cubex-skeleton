{
  "name": "mr-essex/cubex-base",
  "type": "project",
  "description": "Playground for Learning Cubex",
  "license": "MIT",
  "authors": [
    {
      "name": "Kyle Essex",
      "email": "kylegavinessex@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-apcu": "*",
    "cubex/framework": "^4.15.0",
    "filp/whoops": "^2.9.2",
    "mressex/cache": "^0.4",
    "packaged-ui/bem-component": "^1.1",
    "packaged-ui/pagelets": "dev-master",
    "packaged/dispatch": "^2.11.0",
    "packaged/enum": "^1.4.0",
    "packaged/glimpse": "^2.7",
    "packaged/http": "^1.16",
    "packaged/rwd": "^1.4.2",
    "statickidz/php-google-translate-free": "^1.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.4",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "auto",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CubexBase\\Application\\": "src",
      "CubexBase\\StyleGuide\\": "styleguide"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CubexBase\\Tests\\": "tests"
    }
  },
  "minimum-stability": "alpha",
  "scripts": {
    "test": "XDEBUG_MODE=coverage vendor/bin/phpunit"
  }
}
